<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esaunggul.doayuk">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logobk2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logobk2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.esaunggul.doayuk.MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.esaunggul.doayuk.Login"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.esaunggul.doayuk.MateriActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.esaunggul.doayuk.DetailMateriActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.esaunggul.doayuk.JadwalActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>